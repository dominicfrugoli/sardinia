  51:sardinia.cpp  **** void AudioCallback(AudioHandle::InputBuffer in, AudioHandle::OutputBuffer out, size_t size)
  52:sardinia.cpp  **** {
  53:sardinia.cpp  **** 	ProcessControlsKR();
  54:sardinia.cpp  **** 
  55:sardinia.cpp  **** 	for (size_t i = 0; i < size; i++)
  56:sardinia.cpp  **** 	{
  57:sardinia.cpp  **** 		sampleSig = 0; // Reset signal values to 0 in case no sample is being played
  58:sardinia.cpp  **** 		outputMix = 0;
  59:sardinia.cpp  **** 
  60:sardinia.cpp  **** 		ProcessControlsAR();
  61:sardinia.cpp  **** 
  62:sardinia.cpp  **** 		inputSig = (in[0][i] + in[1][i]) * 0.5f;
  63:sardinia.cpp  **** 		
  64:sardinia.cpp  **** 		if(isRecording)
  65:sardinia.cpp  **** 		{
  66:sardinia.cpp  **** 			RecordSample((inputSig)); // Record the input and increment the sample length tracker
  67:sardinia.cpp  **** 			lengthTrack++;
  68:sardinia.cpp  **** 		}
  69:sardinia.cpp  **** 
  70:sardinia.cpp  **** 		if(isPlaying)
  71:sardinia.cpp  **** 		{
  72:sardinia.cpp  **** 			if(stopPoint == 0)
  73:sardinia.cpp  **** 			{
  74:sardinia.cpp  **** 				sampleSig = 0.0f;
  75:sardinia.cpp  **** 			}
  76:sardinia.cpp  **** 			else
  77:sardinia.cpp  **** 			{
  78:sardinia.cpp  **** 				AdvanceReadIndex();
  79:sardinia.cpp  **** 				sampleSig = GetSample();
  80:sardinia.cpp  **** 				if(readIndex > recordingLength) // recordingLength dynamically changes the size of sample based
  81:sardinia.cpp  **** 				{
  82:sardinia.cpp  **** 					readIndex = 0;
  83:sardinia.cpp  **** 				}
  84:sardinia.cpp  **** 			}
  85:sardinia.cpp  **** 		}
  86:sardinia.cpp  **** 
  87:sardinia.cpp  **** 		
  88:sardinia.cpp  **** 		outputMix = inputSig + sampleSig; // Mix the sample playback with the input
  89:sardinia.cpp  **** 		outputMix *= outputAmp;
  90:sardinia.cpp  **** 
  91:sardinia.cpp  **** 		// Output the mixed signals
  92:sardinia.cpp  ****         out[0][i] = outputMix;
  93:sardinia.cpp  **** 		out[1][i] = outputMix;
  94:sardinia.cpp  **** 	}
